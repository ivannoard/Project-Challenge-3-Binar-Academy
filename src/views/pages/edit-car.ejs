<div style="width: 700px;">
  <form action="/edit-car/<%=data[0].id%>" method="post">
    <input type="hidden" id="id" name="id" value='<%=data[0].id%>'>
    <div class="mb-3 row">
      <label for="nama" class="col-sm-2 col-form-label">Nama</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="nama" name="nama" value="<%=data[0].nama%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="harga" class="col-sm-2 col-form-label">Harga</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="harga" name="harga" value="<%=data[0].harga%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="ukuran" class="col-sm-2 col-form-label">Ukuran</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="ukuran" name="ukuran" value="<%=data[0].ukuran%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="foto" class="col-sm-2 col-form-label">Foto</label>
      <div class="col-sm-10">
        <input type="file" class="form-control" id="foto" name="foto" value="<%=data[0].foto%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="startrent" class="col-sm-2 col-form-label">Start Rent</label>
      <div class="col-sm-10">
        <input type="date" class="form-control" id="startrent" name="startrent" value="<%=data[0].startrent%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="finishrent" class="col-sm-2 col-form-label">Finish Rent</label>
      <div class="col-sm-10">
        <input type="date" class="form-control" id="finishrent" name="finishrent" value="<%=data[0].finishrent%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="created_at" class="col-sm-2 col-form-label">Created At</label>
      <div class="col-sm-10">
        <input type="hidden" class="form-control" id="created_at" name="created_at" value="<%=data[0].created_at%>">
      </div>
    </div>
    <div class="mb-3 row">
      <label for="updated_at" class="col-sm-2 col-form-label">Updated At</label>
      <div class="col-sm-10">
        <input type="hidden" class="form-control" id="updated_at" name="updated_at" value="<%=data[0].updated_at%>">
      </div>
    </div>
    <button class="btn btn-primary" type="submit">Simpan</button>
  </form>
</div>